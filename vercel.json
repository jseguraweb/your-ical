{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "server.js" },
    { "src": "/download/(.*)", "dest": "server.js" },
    { "src": "/api-docs", "dest": "server.js" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ]
}
